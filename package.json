{
  "name": "vanth_docs_system_backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prestart": "npm run build",
    "start": "node ./dist-server/bin/www.js",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/bin/www",
    "pm2-server": "pm2 start ./dist-server/bin/www.js --name \"backend\"",
    "dev": "npm run env NODE_ENV=development &&  npm-run-all build server",
    "prod": "npm run env NODE_ENV=production &&  npm-run-all pm2-server",
    "prod-build": "npm run env NODE_ENV=production && npm-run-all build",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon",
    "poststart": "node ./node_modules/pm2/bin/pm2 logs"
  },
  "dependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.10",
    "async": "^3.2.0",
    "aws-sdk": "^2.863.0",
    "axios": "^0.21.1",
    "bcrypt": "^5.0.1",
    "compare-versions": "^4.1.3",
    "cors": "^2.8.5",
    "cpf-cnpj-validator": "^1.0.3",
    "crypto-js": "^4.0.0",
    "debug": "~2.6.9",
    "docusign-esign": "^6.5.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.8",
    "esm": "^3.2.25",
    "express": "~4.16.1",
    "fcm-node": "^1.5.2",
    "firebase": "^9.14.0",
    "fs": "0.0.1-security",
    "fs-extra": "^11.2.0",
    "json2csv": "^5.0.6",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^6.6.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "multiparty": "^4.2.3",
    "nodemailer": "^6.5.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "pm2": "^4.5.5",
    "request": "^2.88.2",
    "rimraf": "^3.0.2",
    "socket.io": "^4.5.3",
    "stripe": "^11.5.0",
    "twilio": "^3.57.0",
    "url": "^0.11.0",
    "uuid": "^9.0.0",
    "uuidv4": "^6.2.13",
    "xml2js": "^0.4.23"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "prettier": "2.7.1"
  }
}
